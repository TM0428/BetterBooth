{
  "name": "betterbooth",
  "version": "0.5.3",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build && cp -r src/js dist/js && cp -r src/css dist/css && cp -r _locales dist/_locales",
    "build:watch": "cp -r src/js dist/js && cp -r src/css dist/css && cp -r _locales dist/_locales && vite build --watch",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "sass": "^1.74.1",
    "vue": "^3.2.47",
    "vue-i18n": "^9.10.2",
    "vue-router": "^4.3.2",
    "vuetify": "^3.5.9"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^4.0.0",
    "@mdi/js": "^7.2.96",
    "@rushstack/eslint-patch": "^1.10.2",
    "@vitejs/plugin-vue": "^4.2.1",
    "@vue/eslint-config-prettier": "^9.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.25.0",
    "install": "^0.13.0",
    "npm": "^10.5.2",
    "prettier": "^3.2.5",
    "prettier-plugin-brace-style": "^0.6.0",
    "vite": "^4.5.3"
  }
}
